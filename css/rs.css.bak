@font-face {
    font-family: "runescape-small";
    src: url('RuneScapeSmall.ttf') format("truetype");
}
span.rssmall { 
    font-family: runescape-small, Verdana, Tahoma;
	top: -6px;
    text-shadow: #000 1px 1px;
    position: absolute;
    color: yellow;
    left: 6px;
    font-size: 16px;
	z-index:1080;
}
.rssmall.white { 
    color: white;
}

.tooltip_templates { display: none; }

div.ttitem span.rssmall{
	left: -2px;	
	top: -9px;
}

.grayscaleitem{
	filter: grayscale(100%);
	opacity: 0.3;
}

.regularitem{
	opacity: 1;
}

.clueitemlevel{
	background-color:#424242;
    border: 2px solid #000;
    display: block;
	float:left;
    height:10px;
	width:20px;
	margin:1px;
	margin-top:5px;
}
.clueitemlevel.elite{
	background-color: rgb(102,51,153);
}
.clueitemlevel.hard{
	background-color: rgb(204,51,51);
}
.clueitemlevel.medium{
	background-color: rgb(255,153,0);
}
.clueitemlevel.easy{
	background-color: rgb(0,153,0);
}

span.cluedisplayitemname{
	font-size:20px;
	font-weight:700;
	color:#000;
}

span.cluedisplayitemname a:link, span.cluedisplayitemname a:visited, span.cluedisplayitemname a:hover, span.cluedisplayitemname a:active{color:#000;}
span.cluedisplayitemname a:hover{text-decoration:underline;cursor:pointer;}

