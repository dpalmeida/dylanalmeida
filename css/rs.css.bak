body {
  margin: 0px;
  padding: 0px;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    /* display: none; <- Crashes Chrome on hover */
    -webkit-appearance: none;
    margin: 0; /* <-- Apparently some margin are still there even though it's hidden */
}

/*stats*/
.rstable table {
	border-collapse: collapse;
	/* min-width:100%; */
}

.rstable th{
	background-color: #000000;
	color: #ffffff;
	font-weight:800;
	text-align: center;
	vertical-align:middle;
}

.tablebkg{
	background-color: rgba(29,29,64,0.3);
}

.rstable tbody td {
    height:25px;
	vertical-align:middle;
	border-right: solid 2px #000000; 
	border-left: solid 2px #000000;
	color: #ffffff;
}

.rstable tbody tr:hover td{
    background-color: #ADD8E6;
	color: #000000;
}

.chart-container{
	position: relative; 
	border: solid 2px rgba(0,0,0,0); 
	padding:15px;
	background-color: rgba(29,29,64,0.5);
}

/* Skills Table */
#maincontent{
	margin-top:25px;
	margin-bottom:25px;
}

.skillimg{
    width: 59px;
    position: absolute;
    left: 34px;
    top: 9px;
}

.skillimg.overall {
    left: 14px;
}

.skillimg-sm{
	max-height:50px;
}

.progress-stats {
	margin-bottom: 0px;
    height: 13px;
    border-radius: 0px;
    background-color: rgba(10,10,22,1);
    position: absolute;
    width: 140px;
}
.progress-bar-stats{
	color: #fff;
	line-height:25px;
	height: 100%;
	font-weight: 900;
	text-shadow: -1px -1px 0 #000,
		1px -1px 0 #000,
		-1px 1px 0 #000,
		1px 1px 0 #000;
}
.progress-cell-stats{
    padding: 0px;
    position: absolute;
    bottom: 10px;
    left: 10px;
    height: 13px;
    opacity: 1;
    width: 140px;
}

.skilldisplay{
	background-image: url('../rs/img/SkillBg.png');
    background-repeat: no-repeat;
    background-position: top left;
	background-size: contain;
    width: 200px;
    height: 100px;
	padding-left:15px;
	padding-top:15px;
    border-spacing: 0;
	display: inline-block;
}

.skilldisplay span{
	color:#fff;
	font-weight:bold;
}

span.level {
